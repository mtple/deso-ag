{
  "name": "deso-ag",
  "version": "1.0.4",
  "description": "CLI tool for aggregating posts from decentralized social protocols (Farcaster, Lens, Nostr, Bluesky)",
  "main": "dist/lib.js",
  "bin": {
    "deso-ag": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/lib.js",
      "types": "./dist/lib.d.ts"
    }
  },
  "type": "module",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "pnpm build",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "farcaster",
    "lens",
    "nostr",
    "bluesky",
    "atproto",
    "decentralized",
    "social",
    "aggregator",
    "cli",
    "web3"
  ],
  "author": "Matt Lee",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mtple/deso-ag"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "nostr-tools": "^2.7.0",
    "websocket-polyfill": "^0.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.23.0+sha512.21c4e5698002ade97e4efe8b8b4a89a8de3c85a37919f957e7a0f30f38fbc5bbdd05980ffe29179b2fb6e6e691242e098d945d1601772cad0fef5fb6411e2a4b"
}
